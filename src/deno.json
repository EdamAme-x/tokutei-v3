{
  "tasks": {
    "deno:dev": "deno run -A npm:astro dev",
    "deno:start": "deno run -A npm:astro dev",
    "deno:build": "deno run -A npm:astro build --remote",
    "deno:preview": "deno run -A npm:astro preview",
    "deno:check": "deno run -A npm:astro check"
  },
  "unstable": [
    "kv",
    "byonm"
  ],
  "imports": {
    "@/": "./src/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": [
      "deno.window",
      "deno.dom",
      "deno.unstable"
    ]
  }
}
